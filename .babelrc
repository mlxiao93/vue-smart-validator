{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7", "Firefox >= 20"]
      },
      "es2015": {
        "modules": false
      }
    }]
  ],
  "plugins": [
    "external-helpers",
    "transform-runtime",
    "transform-object-assign",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}